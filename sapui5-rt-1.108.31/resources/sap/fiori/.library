<?xml version="1.0" encoding="UTF-8" ?>
<library xmlns="http://www.sap.com/sap.ui.library.xsd" >

	<name>sap.fiori</name>
	<vendor>SAP SE</vendor>
	<copyright>SAPUI5
 *
 * (c) Copyright 2009-2022 SAP SE. All rights reserved</copyright>
	<version>1.108.31</version>
	
	<documentation>A hybrid UILibrary merged from the most common UILibraries that are used in Fiori apps</documentation>
	
	<dependencies>
		<dependency>
			<libraryName>sap.ui.core</libraryName>
		</dependency>
	</dependencies>

	<appData>
		<packaging xmlns="http://www.sap.com/ui5/buildext/packaging" version="2.0" >
			<all-in-one>
				<!-- exclude all merged files and the raw config module (only needed in custom bundles) -->
				<exclude name="sap/fiori/appruntime-*" />
				<exclude name="sap/fiori/core-*" />
				<exclude name="sap/fiori/flp-controls-*" />
				<exclude name="sap/fiori/_configurePreloadLibCss.js" />
			</all-in-one>
		</packaging>
		<jsdoc xmlns="http://www.sap.com/ui5/buildext/jsdoc" >
			<exclude name="sap.fiori." />
		</jsdoc>
	</appData>

</library>
